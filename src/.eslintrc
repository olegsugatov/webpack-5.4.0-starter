{
    "parser": "babel-eslint",
    "rules": {
        "no-unused-vars": "warn",
        "semi": "off",
        "arrow-parens": "off",
        "comma-dangle": "off",
        "linebreak-style": "off",
        "quotes": "off",
        "requiere-jsdoc": "off",
        "max-len": "off",
        "operator-linebreack": "off"
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": ["eslint:recommended"]
}